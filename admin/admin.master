        {
            background-color: Transparent;
            border: black 3px solid;
            border-radius: 10px;
            font-size: 15px;
            padding: 5px;
            margin-top: 10px;
            font-weight: bold;
            text-transform: capitalize;
        }
        
        .btn:hover
        {
            color: White;
            background-color: Black;
        }
        
        .TxtInput
        {
            margin: 0 10px 0 5px;
            text-transform: capitalize;
            font-size: 18px;
            border: none;
            padding: 2px;
            border-bottom: 3px solid black;
        }
        
        .error
        {
            background-color: Yellow;
            color: Red;
            font-size: 15px;
            font-weight: 100;
            text-transform: uppercase;
        }
